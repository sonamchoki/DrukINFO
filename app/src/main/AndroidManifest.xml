<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.karma.splashscreen">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity_SPLASHscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Navigation_MainActivity"
            android:label="@string/title_activity_navigation__main"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AboutUs_activity"
            android:parentActivityName=".Navigation_MainActivity" />
        <activity
            android:name=".Rate_us_Avtivity"
            android:parentActivityName=".Navigation_MainActivity" />
        <activity android:name=".List_Activity_Thimphu"
                  android:parentActivityName=".Navigation_MainActivity"
            />
        <activity android:name=".Festival"
                  android:parentActivityName=".List_Activity_Thimphu"
            />
        <activity android:name=".Population_and_Languages"
                  android:parentActivityName=".List_Activity_Thimphu"
            ></activity>
    </application>

</manifest>